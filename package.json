    "server:status": "powershell -ExecutionPolicy Bypass -File server.ps1 status"
  },
  "keywords": ["school", "education", "sma", "ketambe", "aceh"],
  "author": "SMAN 1 Ketambe",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "path": "^0.12.7",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
