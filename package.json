{
  "name": "sman1-ketambe-website",
  "version": "1.0.0",
  "description": "Website for SMA Negeri 1 Ketambe",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "fix-permissions": "node fix-image-permissions.js",
    "server:start": "powershell -ExecutionPolicy Bypass -File server.ps1 start",
    "server:stop": "powershell -ExecutionPolicy Bypass -File server.ps1 stop",
